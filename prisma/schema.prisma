// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = env("DATABASE_URL")
}

model Repository {
  id Int @id @default(autoincrement())
  name String @unique
  url String
  image String?
  description String?
  language String?
  languages String
  homepage String?
}
